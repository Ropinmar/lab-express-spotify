<div class="container">
    <div class="container-albums-results">
        <div class="albums-list">
            {{#each albums}}
                <div class="each-album">
                    <h4>{{name}}</h4>
                    <img src="{{images.1.url}}" class="image-artist">
                    <a href="/tracks/{{this.id}}"><button>View tracks</button></a>
                </div>
            {{/each}}
        </div>
    </div>    
    
</div>